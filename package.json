{
  "name": "@gerard2p/vault-orm",
  "version": "0.4.0",
  "description": "cross orm for node",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "doc": "typedoc --options .typedoc.json --navigation-label-externals \"Internal API\"",
    "test": "mocha test/*.test.ts --timeout 5000",
    "test:live": "mocha test/*.test.ts --timeout 5000 --watch --watch-extensions ts",
    "cover": "nyc npm t"
  },
  "author": "Gerardo Pérez Pérez <gerard2perez@outlook.com>",
  "homepage": "https://github.com/gerard2p/vault-orm#README.md",
  "license": "MIT",
  "files": [
    "*.d.ts",
    "**/*.js",
    "!resolvenextversion.js",
    "LICENSE"
  ],
  "dependencies": {
    "@mysql/xdevapi": "^8.0.15",
    "debug": "^4.1.1",
    "inflection": "^1.12.0",
    "mongodb": "^3.2.3",
    "reflect-metadata": "^0.1.13",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "mongodb": "^3.2.3",
    "@mysql/xdevapi": "^8.0.15"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/inflection": "^1.5.28",
    "@types/mocha": "^5.2.6",
    "@types/mongodb": "^3.1.23",
    "@types/node": "^11.13.4",
    "chai": "^4.2.0",
    "mocha": "^6.1.2",
    "nyc": "^13.3.0",
    "ts-node": "^8.0.3",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typedoc-plugin-ui-router": "^2.0.0",
    "typescript": "^3.4.3"
  },
  "nyc": {
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 60,
    "reporter": [
      "lcov",
      "text"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [],
    "extension": [
      ".ts"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": true,
    "check-coverage": true,
    "all": true,
    "report-dir": "./coverage"
  }
}
