{
  "name": "@bitsun/vault-orm",
  "version": "0.4.0",
  "description": "cross orm for node",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "doc": "typedoc --options .typedoc.json --navigation-label-externals \"Internal API\"",
    "prebuild": "npx tsc",
    "build": "npx releasenext gemfury",
    "postbuild": "git push",
    "gemfury": "releasenext gemfury",
    "version": "releasenext version",
    "test": "mocha test/*.test.ts --timeout 5000",
    "test:live": "mocha test/*.test.ts --timeout 5000 --watch --watch-extensions ts",
    "cover": "nyc npm t"
  },
  "author": "Gerardo Pérez Pérez <gerard2perez@outlook.com>",
  "homepage": "https://github.com/gerard2p/vault-orm#README.md",
  "license": "MIT",
  "files": [
    "*.d.ts",
    "**/*.js",
    "!resolvenextversion.js",
    "LICENSE"
  ],
  "dependencies": {
    "@types/mongodb": "^3.1.17",
    "debug": "^4.1.0",
    "inflection": "^1.12.0",
    "reflect-metadata": "^0.1.12",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@mysql/xdevapi": "^8.0.13",
    "@types/chai": "^4.1.7",
    "@types/inflection": "^1.5.28",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.12",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "mongodb": "^3.1.10",
    "nyc": "^13.1.0",
    "releasenext": "git+https://github.com/gerard2p/releasenext.git",
    "ts-node": "^7.0.1",
    "typedoc": "^0.12.0",
    "typedoc-plugin-external-module-name": "^1.1.3",
    "typedoc-plugin-ui-router": "^1.0.6",
    "typescript": "^3.2.2"
  },
  "nyc": {
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "reporter": [
      "lcov",
      "text"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [],
    "extension": [
      ".ts"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": true,
    "check-coverage": true,
    "all": true,
    "report-dir": "./coverage"
  }
}